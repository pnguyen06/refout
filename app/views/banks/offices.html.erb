<div class="container">
  <p id="notice"><%= notice %></p>

  <%= render 'layouts/bankheader' %>
  <div class="row">
    <div class="col-md-2 col-md-offset-1">
      <div class="row">
        <div class="col-md-12">
          <div class="panel panel-default">
            <div class="panel-body">
              <%= render 'layouts/banksidenav' %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-md-8">
    	<div class="panel panel-default">
				<div class="panel-body">
					<h5><strong>OFFICES</strong></h5>
					<% @offices.each do |office| %>
						<%= office.name %>
					<% end %>
    		</div>
    	</div>
    </div>
  </div>
</div>
