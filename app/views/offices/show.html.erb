<div class="container">
  <p id="notice"><%= notice %></p>

  <%= render 'layouts/bankheader' %>
  <div class="row">
    <div class="col-md-2 col-md-offset-1">
      <div class="row">
        <div class="col-md-12">
          <div class="panel panel-default">
            <div class="panel-body">
              <%= render 'layouts/banksidenav' %>
            </div>
          </div>
        </div>
      </div>
    </div>
    <div class="col-md-8">
      <div class="panel panel-default">
        <div class="panel-body">
<p>
  <strong>Name:</strong>
  <%= @office.name %>
</p>

<p>
  <strong>Description:</strong>
  <%= @office.description %>
</p>

<p>
  <strong>Bank:</strong>
  <%= @office.bank_id %>
</p>

<p>
  <strong>Street:</strong>
  <%= @office.street %>
</p>

<p>
  <strong>City:</strong>
  <%= @office.city %>
</p>

<p>
  <strong>Phone:</strong>
  <%= @office.phone %>
</p>

</div>
      </div>
    </div>
  </div>
